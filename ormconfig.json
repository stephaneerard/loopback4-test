[
  {
    "name": "default",
    "type": "sqlite",
    "database": "./db.sql",
    "synchronize": true,
    "logging": true,
    "entities": [
      "src/models/*.ts"
    ],
    "subscribers": [
      "src/subscriber/*.ts"
    ],
    "migrations": [
      "src/migration/*.ts"
    ],
    "cli": {
      "entitiesDir": "src/model",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
    }
  }
]